{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "check": "turbo run check && npm run format",
    "compile": "turbo run compile",
    "format:fix": "prettier --write \"./**/*.{js,jsx,ts,tsx,md,json}\"",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "prepare-commit-msg": "npx ts-node -T scripts/prepareCommitMessage.ts",
    "pre-commit": "npm run validateBranch && npm run format:fix",
    "pre-push": "npm run check",
    "prepare": "husky install",
    "validateBranch": "npx ts-node -T scripts/validateBranchName.ts"
  },
  "devDependencies": {
    "eslint": "^8.52.0",
    "husky": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tsconfig": "*",
    "turbo": "^1.10.16"
  },
  "name": "rooms",
  "packageManager": "npm@10.2.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "validateGitNames": {
    "pattern": "^(main|dev|staging|qa|test){1}$|^((fix|feat|chore|refactor|docs|style){1}/rms-[0-9]{1,}/[a-z0-9-]{4,120})$",
    "feedback": "Branch name should follow the pattern: '^(main|dev|staging|qa|test){1}$|^((fix|feat|chore|refactor|docs|style){1}/rms-[0-9]{1,}/[a-z0-9-]{4,120})$'",
    "ticketNumberMinLength": 1,
    "ticketPrefix": "rms"
  }
}
