{
  "name": "utils",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "check": "npm-run-all lint compile --parallel",
    "compile": "tsc --build --clean",
    "lint": "eslint . ",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "pre-commit": "lint-staged"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "tsconfig": "*",
    "types": "*",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.{ts,md}": [
      "prettier --write"
    ],
    "*.ts": [
      "eslint -c .eslintrc.js"
    ]
  }
}
