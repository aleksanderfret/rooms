{
  "name": "web-e2e-tests",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "check": "npm-run-all lint compile",
    "compile": "tsc --build --clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pre-commit": "lint-staged",
    "test:dev:e2e": "playwright test",
    "test:e2e": "start-server-and-test \"(cd ../../apps/web && npm run start)\" http://localhost:3000 \"playwright test\""
  },
  "dependencies": {
    "api": "*",
    "start-server-and-test": "^2.0.2",
    "web": "*"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "dotenv": "^16.3.1",
    "eslint-config-custom": "*",
    "lint-staged": "^15.1.0",
    "playwright": "^1.39.0",
    "tsconfig": "*",
    "types": "*"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc.js"
    ]
  }
}
